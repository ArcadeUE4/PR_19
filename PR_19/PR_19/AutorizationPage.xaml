<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PR_19.AutorizationPage">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Регистрация" HorizontalOptions="Center"/>
            <Entry x:Name ="entryLogin" Text="Введите логин"/>
            <Entry x:Name ="entryPassword" Text="Введите пароль" />
            <Button Text="Зарегистроватся" Clicked="OnButtonClicked"/>
            <Button Text="Назад" Clicked="OnButtonReturn"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>